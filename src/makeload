#!/usr/bin/ksh -p

########################################################################
#
#	make the ict.a lib with OPT=LOAD
#
#	$Id: makeload,v 1.7 2001/02/21 16:20:29 jw Exp $
#
########################################################################

if [[ -f Make_tmp ]]; then
    echo 'Error makeload: Make_tmp exists as file ??? - nothing made' >&2
    exit 1;
fi
test -d Make_tmp || mkdir Make_tmp;
mv link.o rsff.o scan.o Make_tmp
if [[ -d LoadObjs ]]; then
    mv LoadObjs/* .
else
    mkdir LoadObjs
fi
make OPT='-DLOAD -DTCP' ict.a
mv ict.o link.o rsff.o scan.o LoadObjs
mv Make_tmp/* .
rmdir Make_tmp
