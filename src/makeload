#!/usr/bin/ksh -p

########################################################################
#
#	make the load.a lib with OPT=LOAD
#
#	$Id: makeload,v 1.4 2000/06/10 11:27:58 jw Exp $
#
########################################################################

if [[ -e Make_tmp ]]; then echo 'Make_tmp exists ??? - nothing made'; exit 1; fi
mkdir Make_tmp
mv pptc.o link.o rsff.o scan.o Make_tmp
if [[ -d LoadObjs ]]; then
    mv LoadObjs/* .
else
    mkdir LoadObjs
fi
make OPT='-DLOAD -DTCP' load.a
mv pptc.o link.o rsff.o scan.o LoadObjs
mv Make_tmp/* .
rmdir Make_tmp
