#!/usr/bin/ksh -p

########################################################################
#
#	make pplc pplt pptc load.a
#
#	$Id: makeall,v 1.2 2001/01/06 16:56:57 jw Exp $
#
########################################################################

echo "make pplc"
make pplc 2>&1 | grep -iE '(error|warning|:[0-9]+:)'
#echo "make pplc"
# make pplt 2>&1 | grep -iE '(error|warning|:[0-9]+:)'
echo "make pptc"
makepptc 2>&1 | grep -iE '(error|warning|:[0-9]+:)'
echo "make load.a"
makeload 2>&1 | grep -iE '(error|warning|:[0-9]+:)'
