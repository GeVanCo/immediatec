BEGIN {
	name = "iC_ID";
	x = "";
	mod = "";
    }

/\$Id/ {
	printf "%sconst char %s%s[] =\n", mod, name, x;
	x++;
	mod = "static ";
	printf "\t\"@%s%s\";\n", "(#)", $0;
	if (x <= 1) {
	    printf("#ifndef LOAD\n");
	}
    }

END {
	printf("#endif\n");
	printf "%sconst char %s%s[] =\n", mod, name, x;
	printf "\t\"@(#) $Id: pawk.a,v 1.4 2005/01/24 11:39:34 jw Exp $\";\n";
    }
